                    <%end%>
                </div>
                
         
            </div>


      
        </div>


        
        <div class="col-lg-4 col-sm-12 col-form-progress">
   <div class="container-window processpart">
                <div class="left-space">
                    <div class="circle-p"></div>
                </div>
                <div class="process-list">
                    <p>Tu checklist antes de lanzar:</p>

                    <ul>
                        <li class="true" ><i class="fa fa-check"></i> Nombre comercial</li>
                        <li class="true" ><i class="fa fa-check"></i>Especialización</li>
                        <li class="true" ><i class="fa fa-check"></i>Imagen de portada</li>
                        <li class="false" ><i class="fa fa-times"></i>Tus soluciones</li>
                        <li class="false" ><i class="fa fa-times"></i>Tus resultados</li>
                    </ul>
                </div>
            </div>

        </div>
       
    </div>
