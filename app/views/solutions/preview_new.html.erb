<div class="container-body-form container">

    <div class="row">
        
        <div class="col-lg-8 col-sm-12 col-form-cont">
            
            <div class="container-window">
                <div class="menu-editor-solu">
                    <ul>
                        <a class=" linkactivepage" href=""> <li>Soluciones</li></a>
                        <a class="hvr-underline-from-left" href=""> <li>Prácticas Recomendadas</li></a>
                    </ul>
                </div>

                <div class="button-center">
                    <%= link_to new_solution_path do %>
